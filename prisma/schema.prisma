

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("VITE_DATABASE_URL")
}

model posts {
  id          String  @id @default(cuid())
  description String? 
  title       String? 
  email       String?
}

model users {
  id       String  @id @default(cuid())
  email    String  @unique
  username String 
}
